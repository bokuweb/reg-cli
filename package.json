{
  "name": "reg-cli",
  "version": "0.10.5",
  "description": "",
  "main": "./dist/index.js",
  "start": "node ./dist/cli.js",
  "scripts": {
    "watch": "babel src --watch -d dist",
    "build": "npm run build:cli && npm run build:report",
    "build:cli": "babel src -d dist",
    "build:report": "npm run copy:ximgdiff && webpack",
    "dev:report": "webpack-dev-server --hot --inline --open",
    "flow": "flow",
    "copy:ximgdiff": "copyfiles -u 3 node_modules/x-img-diff-js/build/cv-wasm_browser.* report/assets",
    "prepublish": "npm run build",
    "reg": "node dist/cli.js ./sample/actual ./sample/expected ./sample/diff -I -R ./sample/index.html -T 0.01 -X client",
    "screenshot": "node test/screenshot.js",
    "test:cli": "chmod +x dist/cli.js && avaron test/cli.test.js",
    "test:report": "avaron test/report/*.test.js --renderer",
    "test:screenshot": "npm run build:report && npm run reg && npm run screenshot",
    "test": "npm run build:report && npm run build && npm run test:cli && npm run test:report && npm run test:screenshot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reg-viz/reg-cli/"
  },
  "author": "bokuweb",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^2.1.0",
    "cli-spinner": "^0.2.6",
    "cross-spawn": "^5.1.0",
    "glob": "^7.1.2",
    "img-diff-js": "^0.4.0",
    "lodash": "^4.17.4",
    "make-dir": "^1.0.0",
    "md5-file": "^3.1.1",
    "meow": "^3.7.0",
    "mustache": "^2.3.0",
    "x-img-diff-js": "^0.1.4"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "avaron": "^0.0.13",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.2",
    "flow-bin": "^0.54.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.0",
    "nightmare": "^2.10.0",
    "reg-keygen-git-hash-plugin": "^0.5.1",
    "reg-notify-github-plugin": "^0.5.1",
    "reg-publish-s3-plugin": "^0.6.0",
    "reg-suit": "^0.5.1",
    "require-extension-hooks": "^0.3.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.0",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.24",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "vue": "^2.3.3",
    "vue-lazyload": "^1.0.3",
    "vue-loader": "~12.2.2",
    "vue-template-compiler": "^2.3.3",
    "vue-thin-modal": "^0.2.0",
    "webpack": "~3.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "files": [
    "dist",
    "template",
    "report"
  ],
  "bin": {
    "reg-cli": "./dist/cli.js"
  },
  "avaron": {
    "require": [
      "./test/helpers/setup.js"
    ],
    "fixture": "fixture.html"
  }
}
